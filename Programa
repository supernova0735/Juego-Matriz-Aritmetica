#Importando Bibliotecas
import tkinter as tk


#Creando Pantalla de inicio
menuinicio = tk.Tk()
menuinicio.title("Inicio")

#Generando Pantalla de inico
lienzoinicio = tk.Canvas(menuinicio, width=400, height=400)
lienzoinicio.configure(bg="#D2F8BB")
lienzoinicio.pack()

menuinicio.mainloop()
